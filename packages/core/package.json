{
  "name": "@owntwin/core",
  "version": "0.1.0",
  "type": "module",
  "author": "Kentaro Ozeki",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "(tsc || exit 0) && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.cjs ./src/index.ts ./src/components/index.ts",
    "preview": "vite preview"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "require": "./dist/components/*.cjs"
    },
    "./lib/utils": {
      "import": "./dist/lib/utils.js",
      "require": "./dist/lib/utils.cjs"
    },
    "./lib/components": {
      "import": "./dist/lib/components/index.js",
      "require": "./dist/lib/components/index.cjs"
    },
    "./lib/components/*": {
      "import": "./dist/lib/components/*.js",
      "require": "./dist/lib/components/*.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "store": [
        "./dist/store.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ],
      "components/*": [
        "./dist/components/*"
      ],
      "lib/components": [
        "./dist/lib/components/index.d.ts"
      ],
      "lib/*": [
        "./dist/lib/*"
      ],
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "@react-three/fiber": "^8.9.1",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "three": "0.147.0"
  },
  "dependencies": {
    "@mapbox/sphericalmercator": "^1.2.0",
    "@react-three/drei": "9.56.6",
    "camera-controls": "^1.37.4",
    "hold-event": "^0.2.0",
    "jotai": "^1.11.2",
    "just-debounce-it": "^3.2.0",
    "just-split": "^3.2.0"
  },
  "devDependencies": {
    "@react-three/fiber": "^8.9.1",
    "@types/geojson": "^7946.0.10",
    "@types/node": "^12.20.55",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/three": "0.147.0",
    "@vitejs/plugin-react": "^3.0.0",
    "dts-bundle-generator": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "three": "0.147.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1"
  }
}
